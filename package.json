{
	"name": "twofactor_webauthn",
	"version": "0.3.0",
	"description": "WebAuthn second factor provider for Nextcloud",
	"private": true,
	"dependencies": {
		"@nextcloud/auth": "^1.3.0",
		"@nextcloud/axios": "^1.9.0",
		"@nextcloud/initial-state": "^1.2.1",
		"@nextcloud/logger": "^2.1.0",
		"@nextcloud/password-confirmation": "^1.0.1",
		"@nextcloud/router": "^2.0.0",
		"@nextcloud/vue": "^5.1.0",
		"vue": "^2.6.14",
		"vue-click-outside": "^1.1.0",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.17.5",
		"@babel/preset-env": "^7.16.11",
		"@nextcloud/eslint-config": "^7.0.2",
		"@vue/babel-preset-app": "^5.0.1",
		"@vue/test-utils": "^1.3.0",
		"babel-loader": "^8.2.3",
		"babel-plugin-istanbul": "^6.1.1",
		"chai": "^4.3.6",
		"coveralls": "^3.1.1",
		"css-loader": "^6.6.0",
		"file-loader": "^6.2.0",
		"istanbul-instrumenter-loader": "^3.0.1",
		"jsdom": "^19.0.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^9.2.1",
		"mochapack": "^2.1.4",
		"nyc": "^15.1.0",
		"prettier": "^2.5.1",
		"sinon": "^13.0.1",
		"url-loader": "^4.1.1",
		"vue-loader": "^15.9.8",
		"vue-template-compiler": "^2.6.14",
		"webpack": "^5.69.1",
		"webpack-cli": "^4.9.2",
		"webpack-merge": "^5.8.0",
		"webpack-node-externals": "^3.0.0"
	},
	"scripts": {
		"dev": "webpack --progress --config src/webpack.dev.js",
		"watch": "webpack --progress --watch --config src/webpack.dev.js",
		"build": "webpack --progress --config src/webpack.prod.js",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"lint": "eslint --ext .js,.vue src",
		"lint:fix": "eslint --ext .js,.vue src --fix",
		"test": "NODE_ENV=test nyc mochapack --webpack-config src/webpack.test.js --require src/tests/setup.js src/tests/**/*.spec.js",
		"test:watch": "mochapack -w --webpack-config src/webpack.test.js --require src/tests/setup.js src/tests/**/*.spec.js"
	},
	"browserslist": [
		"last 2 versions",
		"ie >= 11"
	],
	"engines": {
		"node": "^14.0.0",
		"npm": "^7.0.0"
	},
	"jshintConfig": {
		"esversion": 6
	},
	"nyc": {
		"sourceMap": false,
		"instrument": false,
		"reporter": [
			"lcov",
			"text-summary"
		],
		"extension": [
			".js",
			".vue"
		],
		"exclude": [
			"js",
			"src/tests",
			"src/webpack.*"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nextcloud/twofactor_webauthn.git"
	},
	"author": "Christoph Wurst",
	"license": "AGPL-3.0",
	"bugs": {
		"url": "https://github.com/nextcloud/twofactor_webauthn/issues"
	},
	"homepage": "https://github.com/nextcloud/twofactor_webauthn#readme"
}
